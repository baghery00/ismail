<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="account_asset_view_form" model="ir.ui.view">
        <field name="name">account.asset.form.inherit.month.based</field>
        <field name="model">account.asset</field>
        <field name="inherit_id" ref="account_asset_management.account_asset_view_form" />
        <field name="arch" type="xml">
            <field name="prorata" position="after">
                <field name="month_based" />
            </field>
            <field name="partner_id" position="after">
                <field name="move_id" />
            </field>
        </field>
    </record>

</odoo>
